{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}",
                "/I",
                "${workspaceFolder}\\curl\\include",
                "/I",
                "${workspaceFolder}\\json-c\\include",
                "/MD",
                "ws2_32.lib","wldap32.lib","advapi32.lib","kernel32.lib","crypt32.lib",
                "${workspaceFolder}\\curl\\lib\\x86\\libcurl.lib",
                "${workspaceFolder}\\json-c\\lib\\x86\\json-c-static.lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}